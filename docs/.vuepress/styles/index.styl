.navbar{
  background-color:#19263B;
  min-height: 48px;
  font-size: 16px;
  .home-link .site-name {
      color: #fff;
  }
}
#app .links {
    color: #fff;
    font-size: 16px;
    background: transparent;
}
table {
  width: 100%; /*表格宽度*/
  table-layout: automatic;
  display: table;
}
table th {
  white-space: nowrap; /*表头内容强制在一行显示*/
  min-width: 80px;
  text-align:left;
}
td, th {
  border: 1px solid #dfe2e5;
  padding: .6em 0.6em;
}
pre{
  word-break: break-all !important;
  code > span{
    white-space: normal
  }
}
.nav-item{
  color: #fff;
  a::after{
    content:'';
    margin:0 5px;
    width:20px;
    height:20px;
    display:inline-block;
    background:url("data:image/png;base64,");
    background-size:cover;
  }
  a span{
    display:none;
  }
  a:hover{
    color: #fff !important;
  }
}